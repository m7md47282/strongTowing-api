                let currentValue = phoneNumber.value;

                // إذا كانت القيمة فارغة، إعادة مفتاح الدولة فقط
                if (currentValue.trim() === '') {
                    phoneNumber.value = areaCode.value;
                    return;
                }

                const escapedAreaCode = areaCode.value.replace(/([.*+?^=!:${}()|\[\]\/\\])/g, "\\$1");

